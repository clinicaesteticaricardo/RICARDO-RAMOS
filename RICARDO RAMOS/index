    <script>
        // Mobile Menu Toggle
        const menuToggle = document.querySelector('.menu-toggle');
        const navLinks = document.querySelector('.nav-links');

        menuToggle?.addEventListener('click', () => {
            navLinks.classList.toggle('active');
        });

        document.querySelectorAll('.nav-links a').forEach(link => {
            link.addEventListener('click', () => {
                navLinks.classList.remove('active');
            });
        });

        // Carousel
        class Carousel {
            constructor(element) {
                this.element = element;
                this.slides = element.querySelectorAll('.carousel-slide');
                this.prevBtn = element.querySelector('.prev');
                this.nextBtn = element.querySelector('.next');
                this.currentIndex = 0;
                
                if (this.slides.length > 0) {
                    this.slides[0].classList.add('active');
                    if (this.prevBtn && this.nextBtn) {
                        this.prevBtn.addEventListener('click', () => this.prev());
                        this.nextBtn.addEventListener('click', () => this.next());
                    }
                    setInterval(() => this.next(), 5000);
                }
            }

            show(index) {
                this.slides.forEach(s => s.classList.remove('active'));
                this.slides[index].classList.add('active');
            }

            next() {
                this.currentIndex = (this.currentIndex + 1) % this.slides.length;
                this.show(this.currentIndex);
            }

            prev() {
                this.currentIndex = (this.currentIndex - 1 + this.slides.length) % this.slides.length;
                this.show(this.currentIndex);
            }
        }

        document.querySelectorAll('.carousel').forEach(el => new Carousel(el));

        // Accordion
        document.querySelectorAll('.accordion-item').forEach(item => {
            const header = item.querySelector('.accordion-header');
            header.addEventListener('click', () => {
                const isActive = item.classList.contains('active');
                
                document.querySelectorAll('.accordion-item').forEach(i => {
                    i.classList.remove('active');
                    i.querySelector('.accordion-body').style.maxHeight = null;
                });

                if (!isActive) {
                    item.classList.add('active');
                    const body = item.querySelector('.accordion-body');
                    body.style.maxHeight = body.scrollHeight + "px";
                }
            });
        });

        // Contact Form → WhatsApp
        document.getElementById('contactForm')?.addEventListener('submit', e => {
            e.preventDefault();
            const name = document.getElementById('name').value.trim();
            const email = document.getElementById('email').value.trim();
            
            if (!name || !email.includes('@')) {
                alert('Por favor, preencha nome e um e-mail válido!');
                return;
            }

            const message = `Olá! Meu nome é *${name}* (${email}). Gostaria de agendar uma avaliação na clínica.`;
            window.open(`https://wa.me/5563999610812?text=${encodeURIComponent(message)}`, '_blank');
        });
    </script>
</body>
</html>
